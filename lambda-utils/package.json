  "overrides": {
    "type-fest": "3.3.0"
  },
  "peerDependencies": {
    "@middy/core": "4.x.x",
    "@middy/http-cors": "4.x.x",
    "@middy/http-event-normalizer": "4.x.x",
    "@middy/http-header-normalizer": "4.x.x",
    "@middy/http-json-body-parser": "4.x.x",
    "@sailplane/logger": ">=5.0.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ]
