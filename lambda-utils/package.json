  "repository": {
    "type": "git",
    "url": "https://github.com/onicagroup/sailplane.git"
  },
  "license": "Apache-2.0",
  "author": "Onica Group",
  "homepage": "https://docs.onica.com/projects/sailplane",
  "contributors": [
    "Adam Fanello <adam.fanello@rackspace.com>"
  ],
  "devDependencies": {
    "@middy/core": "^4.7.0",
    "@middy/http-cors": "^4.7.0",
    "@middy/http-event-normalizer": "^4.7.0",
    "@middy/http-header-normalizer": "^4.7.0",
    "@middy/http-json-body-parser": "^4.7.0",
    "@sailplane/logger": "file:../logger",
    "@types/aws-lambda": "^8.10.134",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.17",
    "http-errors": "^1.8.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "4.9.x"
